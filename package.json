{
  "name": "facehub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "mocha --require react-native-mock/mock.js --compilers js:babel-core/register --recursive test/**/*.js",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "build-ios": "react-native unbundle --entry-file index.ios.js --platform ios --dev false",
    "build-android": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk && cd ..",
    "checkversion": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "postinstall": "npm run checkversion",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretest": "npm run lint",
    "prestart": "npm run lint"
  },
  "dependencies": {
    "react": "15.0.2",
    "react-native": "0.26.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-native-mock": "^0.2.2",
    "sinon": "^1.17.4"
  },
  "devEngines": {
    "node": ">= 4.x",
    "npm": ">= 3.x"
  }
}
